{
  "name": "@raincatcher/datasync-cloud",
  "version": "1.0.0",
  "description": "Raincatcher DataSync cloud",
  "types": "src/index.ts",
  "author": "feedhenry-raincatcher@redhat.com",
  "license": "Apache-2.0",
  "scripts": {
    "clean": "del src/**/*.js src/**/*.map test/**/*.js test/**/*.map",
    "build": "tsc",
    "test": "nyc mocha",
    "prepublish": "npm run clean && npm run build"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "check-coverage": true,
    "lines": 75,
    "functions": 100,
    "branches": 50
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.3",
    "@types/body-parser": "^1.16.3",
    "@types/bunyan": "0.0.36",
    "@types/cors": "^2.8.1",
    "@types/express": "^4.0.35",
    "@types/mocha": "^2.2.41",
    "@types/node": "^7.0.14",
    "@types/proxyquire": "^1.3.27",
    "proxyquire": "^1.8.0",
    "del-cli": "^1.0.0",
    "mocha": "^3.4.2",
    "nyc": "^11.0.1",

    "source-map-support": "^0.4.15",
    "ts-node": "^3.0.4",
    "typescript": "^2.3.4"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "cors": "^2.8.3",
    "express": "^4.14.0",
    "fh-sync": "^1.0.2"
  }
}
